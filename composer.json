{
  "name": "xmo/mine",
  "license": "Apache-2.0",
  "description": "mineAdmin核心组件",
  "homepage": "http://www.mineadmin.com",
  "keywords": [
    "MineAdmin",
    "hyperf",
    "hyperf-admin",
    "Admin"
  ],
  "autoload": {
    "psr-4": {
      "Mine\\": "src/mine/src",
      "Mine\\Gateway\\": "src/gateway/src",
      "Xmo\\JWTAuth\\": "src/jwt-auth/src",
      "Mine\\Office\\": "src/office/src",
      "Mine\\Translatable\\": "src/translatable/src"
    },
    "files": [
      "src/mine-core/src/Helper/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mine\\Tests\\": "tests"
    }
  },
  "replace": {
    "xmo/mine-core": "*",
    "xmo/mine-gateway": "*",
    "xmo/jwt-auth": "*",
    "xmo/mine-office": "*",
    "xmo/mine-translatable": "*",
    "xmo/mine-helpers": "*",
    "xmo/mine-genertor": "*"
  },
  "authors": [
    {
      "name": "xmo",
      "email": "root@imoi.cn",
      "role": "Developer"
    },
    {
      "name": "DeathSatan",
      "email": "2771717608@qq.com",
      "role": "Developer"
    }
  ],
  "replace": {
    "xmo/mine": "*"
  },
  "require": {
    "php": ">=8.1",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-redis": "*",
    "ext-swoole": ">=5.0",
    "doctrine/dbal": "^3.1",
    "hyperf/di": "~3.1.0",
    "hyperf/amqp": "~3.1.0",
    "hyperf/async-queue": "~3.1.0",
    "hyperf/cache": "~3.1.0",
    "hyperf/codec": "~3.1.0",
    "hyperf/command": "~3.1.0",
    "hyperf/config": "~3.1.0",
    "hyperf/crontab": "~3.1.0",
    "hyperf/database": "~3.1.0",
    "hyperf/database-pgsql": "~3.1.0",
    "hyperf/constants": "~3.1.0",
    "hyperf/db-connection": "~3.1.0",
    "hyperf/filesystem": "~3.1.0",
    "hyperf/flysystem-oss": "^1.2",
    "hyperf/framework": "~3.1.0",
    "hyperf/guzzle": "~3.1.0",
    "hyperf/http-server": "~3.1.0",
    "hyperf/logger": "~3.1.0",
    "hyperf/memory": "~3.1.0",
    "hyperf/model-cache": "~3.1.0",
    "hyperf/paginator": "~3.1.0",
    "hyperf/pool": "~3.1.0",
    "hyperf/process": "~3.1.0",
    "hyperf/redis": "~3.1.0",
    "hyperf/snowflake": "~3.1.0",
    "hyperf/support": "~3.1.0",
    "hyperf/translation": "~3.1.0",
    "hyperf/validation": "~3.1.0",
    "hyperf/websocket-server": "~3.1.0",
    "hyperf/migration-generator": "~3.1.0",
    "overtrue/flysystem-cos": "^5.0",
    "overtrue/flysystem-qiniu": "^3.0",
    "symfony/property-access": "^6.0",
    "symfony/serializer": "^6.0",
    "tangwei/apidocs": "^3.0",
    "yurunsoft/phpmailer-swoole": "^1.0",
    "zoujingli/ip2region": "2.0.*",
    "ramsey/uuid": "^4.7",
    "lcobucci/jwt": "~4.1.0",
    "nesbot/carbon": "^1.0 || ^2.0",
    "phpoffice/phpspreadsheet": "^1.24",
    "hyperf/model-listener": "~3.1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "hyperf/testing": "3.1.*",
    "phpstan/phpstan": "^1.0",
    "swoole/ide-helper": "^5.0",
    "mockery/mockery": "^1.0"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix $1",
    "test": "co-phpunit --prepend test/bootstrap.php -c phpunit.xml --colors=always",
    "analyse": "phpstan analyse --memory-limit 1000M -l 0 -c phpstan.neon src"
  }
}